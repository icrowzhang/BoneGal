{"id":1,"name":"door_toilet","note":"廉租房里的厕所门","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"tileId":0,"characterName":"!Door5","direction":2,"pattern":0,"characterIndex":4},"list":[{"code":101,"indent":0,"parameters":["Package1",0,0,2]},{"code":401,"indent":0,"parameters":["厕所的门……怎么是锁着的？"]},{"code":101,"indent":0,"parameters":["SF_Actor2",4,0,2]},{"code":401,"indent":0,"parameters":["啊，那个啊……那是我的朋友xxx住的地方。"]},{"code":401,"indent":0,"parameters":["他是一个骷髅法师，只有雨季才会过来。"]},{"code":101,"indent":0,"parameters":["Package1",0,0,2]},{"code":401,"indent":0,"parameters":["你的朋友……住在……厕所里？"]},{"code":101,"indent":0,"parameters":["SF_Actor2",4,0,2]},{"code":401,"indent":0,"parameters":["干嘛那个表情啊……他自己都不介意，有个地方呆着就行了。"]},{"code":401,"indent":0,"parameters":["亡灵又用不到厕所，那就只是个房间而已。"]},{"code":101,"indent":0,"parameters":["SF_Actor2",4,0,2]},{"code":401,"indent":0,"parameters":["而且他的房间里放着很多标本、研究素材、法术书之类的东西……"]},{"code":401,"indent":0,"parameters":["反正都挺超出常人理解范围的。"]},{"code":401,"indent":0,"parameters":["我还不想被房东当成变态，所以就干脆把门锁起来了。"]},{"code":123,"indent":0,"parameters":["A",0]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":1,"stepAnime":false,"through":false,"trigger":0,"walkAnime":false},{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":true,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"tileId":0,"characterName":"!Door5","direction":2,"pattern":0,"characterIndex":4},"list":[{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,